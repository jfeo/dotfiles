! urxvt
URxvt*scrollBar:       false
URxvt*scrollBar_right: false
URxvt.font:	           xft:Terminus:pixelsize=14
URxvt.boldFont:		     xft:Terminus:pixelsize=14:antialias=true:bold
URxvt.italic:          xft:Terminus:pixelsize=14:antialias=true:italic
URxvt*shading:         20
URxvt*transparent:     true
URxvt.background:      0000000
URxvt.foreground:      #bbbbbb

! cursor
Xcursor.theme:      Vanilla-DMZ
Xcursor.theme_core: true
Xcursor.size:       0

! black
*color0:        #000000
*color8:        #4d4d4d
! red
*color1:        #D81860
*color9:        #F00060
! green
*color2:        #60FF60
*color10:       #70FF70
! yellow
*color3:        #f9fd75
*color11:       #f9fd80
! blue
*color4:        #30309b
*color12:       #4141aa
! magenta
*color5:        #7e40a5
*color13:       #8a5fa5
! cyan
*color6:        #3579a8
*color14:       #4891c3
! white
*color7:        #f3ebe2
*color15:       #eeeeee
